{% set page_title = "grid" %} 
{% extends "layout.njk" %}
{% block pagetitle %}<title>{{ config.title }}</title>{% endblock %}
{% block layout %}
  <section class="grid">
    {% for x in range(0, 25) %}
    <article><img class="zoom" src="https://source.unsplash.com/random/{{ x }}" alt="Unsplash image"/></article>
    {% endfor %}
  </section> 
{% endblock %}